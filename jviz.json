{
	"name": "jviz-tab",
	"description": "The base table module for jviz",
  "version": "0.0.1",
	"contributors": { "id": "jmjuanes", "name": "Josemi Juanes", "email": "josemijuanes@gmail.com" },
  "directory": "./build",
  "tasks":
  {
    "build:js:main": { "task": "concat", "src": [ "./js/tab.js", "./js/**.js" ], "dest": "./jviz-tab.js" },
    "build:sass": { "task": "compile:sass", "src": "./scss/**.scss", "dest": "./" }
  },
	"build":
  [
    "build:js:main",
    "build:sass"
  ],
	"dependencies": { "jviz": "dev" }
}
